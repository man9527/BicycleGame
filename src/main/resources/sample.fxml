<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="268.0" prefWidth="420.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawi.Controller">
   <columnConstraints>
      <ColumnConstraints maxWidth="196.0" minWidth="190.0" prefWidth="190.0" />
      <ColumnConstraints maxWidth="43.0" minWidth="37.0" prefWidth="43.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="82.0" minHeight="33.0" prefHeight="33.0" />
      <RowConstraints maxHeight="205.0" minHeight="156.0" prefHeight="205.0" />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label fx:id="clock" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Slider fx:id="calBurn" max="100000.0" GridPane.columnIndex="2" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1">
         <image>
            <Image url="@cold.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <image>
            <Image url="@hot.png" />
         </image>
      </ImageView>
      <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2">
         <children>
            <Button mnemonicParsing="false" onAction="#beginUsbValue" text="Start" />
            <Button mnemonicParsing="false" onAction="#stopUsbValue" text="Stop" />
            <Button mnemonicParsing="false" onAction="#newGame" text="New Game" />
         </children>
      </HBox>
   </children>
</GridPane>
